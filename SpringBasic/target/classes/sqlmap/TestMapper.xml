<?xml version="1.0" encoding="UTF-8"?>

<!-- 만들면 txt에 있는 매퍼용 코드 적기-->
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <!-- namespace="구현인터페이스의 경로" -->
  <mapper namespace="com.simple.basic.test.mapper.TestMapper">
  	
  	<!-- id="추상메서드 이름", select, int, map은 resultType이 필요-->
  	<select id="getTime" resultType="string">
  		SELECT SYSDATE AS Time FROM DUAL
  	</select>
  	
  	<!--  > JdbcTest 코드 돌리러 -->
  
  
  
  
  </mapper>
  
  
  
  
  
  
  
  